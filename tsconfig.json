{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@third/*": [
        "src/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@localizations/*": [
        "src/utils/*"
      ],
      "@lotties/*": [
        "src/assets/lotties/*"
      ],
      "@repositories/*": [
        "src/repositories/*"
      ],
      "@services/*": [
        "src/services/*"
      ],
    }
  }
}